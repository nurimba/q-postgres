{
  "extends": "eslint:recommended",

  "plugins": [
    "react"
  ],

  "ecmaFeatures": {
    "jsx": true
  },

  "env": {
    "node": true,
    "browser": false
  },

  "rules": {
    "strict": [2, "global"],
    "camelcase": 2,
    "curly": 0,
    "brace-style": [2, "1tbs"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-in-brackets": [0, "never"],
    "space-infix-ops": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "react/react-in-jsx-scope": 2,
    "react/jsx-uses-react": 2
  }
}
